{
  "name": "ufs",
  "version": "1.0.0",
  "description": "ultra foundation solution",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enforcer-squad/ufs.git"
  },
  "keywords": [
    "foundation",
    "cli",
    "deploy"
  ],
  "author": "enforcer.a06 <enforcer.a06@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enforcer-squad/ufs/issues"
  },
  "homepage": "https://github.com/enforcer-squad/ufs#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "clean": "pnpm --filter=@enforcer-squad/* run clean && rimraf ./node_modules && pnpm i",
    "start": "pnpm --filter=@enforcer-squad/* run  --parallel start",
    "build": "pnpm --filter=@enforcer-squad/* run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/adm-zip": "^0.5.5",
    "@types/node": "^20.10.4",
    "@types/root-check": "^1.0.2",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "adm-zip": "^0.5.10",
    "commander": "^11.1.0",
    "install": "^0.13.0",
    "shelljs": "^0.8.5"
  },
  "lint-staged": {
    "packages/**/src/*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387"
}
